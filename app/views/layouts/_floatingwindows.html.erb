<aside
  id="floating-window-1"
  class="floating-window window98"
  data-distractionmode-target="window"
  style="display: none; transition: transform 0.3s cubic-bezier(0.34, 1.56, 0.64, 1), left 0.3s cubic-bezier(0.34, 1.56, 0.64, 1), top 0.3s cubic-bezier(0.34, 1.56, 0.64, 1), opacity 0.3s ease-in-out; opacity: 0; z-index: 1;"
  role="complementary"
>
  <div class="title-bar">
    <span class="title-bar-text">0_o → Soap cutting video</span>
  </div>
  <div class="window-body">
    <video
      id="vid1"
      width="600"
      height="300"
      class="distracting-video"
      playsinline="true"
      preload="auto"
      fetchpriority="low"
      loading="lazy"
      loop="true"
      data-controller="hls"
      data-stream-url="/streaming/soapcarving_optimised_video"
      x-webkit-airplay="deny"
      disablepictureinpicture="true"
      disableremoteplayback="true"
      data-distractionmode-target="video"
    >
    </video>
  </div>
</aside>

<aside
  id="floating-window-2"
  class="floating-window window98 window-width-override"
  data-controller="doom-cube"
  data-distractionmode-target="window"
  style="display: none; transition: transform 0.3s cubic-bezier(0.34, 1.56, 0.64, 1), left 0.3s cubic-bezier(0.34, 1.56, 0.64, 1), top 0.3s cubic-bezier(0.34, 1.56, 0.64, 1), opacity 0.3s ease-in-out; opacity: 0; z-index: 2;"
  role="complementary"
>
  <div class="title-bar">
    <span class="title-bar-text">The cube of DOOM™</span>
  </div>
  <div class="window-body" data-doom-cube-target="root">
    <canvas
      class="doom-canvas"
      data-doom-cube-target="canvas"
      width="548"
      height="308"
      style="display: block;"
    ></canvas>
    <p>See the
      <a href="/doomdisclaimer" rel="noopener">doom disclaimer</a>
    </p>
  </div>
</aside>

